public class InheritedMemberCall {


	public static void main(String args[]) {
		Parent c = new child();
		
		System.out.println(c.parentFun(c.parentVar1));
		System.out.println(c.childFun()); //This should fail
	}
	
	class Parent {
		public int parentVar1;
		public String ParentVar2;
		
		public String parentFun(int num) { }
	}
	
	class Child extends Parent {
		public int childVar;
		
		public String childFun() { return parentFun(childVar); }
	}
}
